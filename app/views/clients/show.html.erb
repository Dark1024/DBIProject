<div class="container all">
  <div class="col-md-8">
    <div class="row">
      <h3>Cliente</h3>
    </div>    
    <div class="row">
      <p id="notice"><%= notice %></p>

      <p>
        <strong>Numero Id:</strong>
        <%= @client.id_number %>
      </p>
      
      <p>
        <strong>Nombre:</strong>
        <%= @client.name %>
      </p>
      
      <p>
        <strong>Fecha de Nacimiento:</strong>
        <%= @client.birthday %>
      </p>
      
      <%= link_to 'Edit', edit_client_path(@client) %> |
      <%= link_to 'Back', clients_path %>
    </div>   
  </div>    
</div>